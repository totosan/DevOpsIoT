@{
	ViewData["Title"] = "Home Page";
}

<h2>Home</h2>

<ul>
	@{ if (ViewBag.Devices != null)
		{
			foreach (var device in ViewBag.Devices)
			{
				<li><a href="/overview/functions?deviceId=@device.Id">@device.Id</a> - @device.Status</li>
				}
			}
	}
</ul>

<div class="btn">
	@Html.ActionLink("Update all", "UpdateAll")
</div>